def prime_first(): 
    asal=[]
    for sayı in range(1,500):  
        if sayı > 1:  
            for i in range(2,sayı):  
                if (sayı % i) == 0:  
                    break  
            else:  
            #print(sayı)
                asal.append(sayı)
    print(asal)
def prime_second(): 
    asal=[]
    for sayı in range(500,1000):  
        if sayı > 500:  
            for i in range(2,sayı):  
                if (sayı % i) == 0:  
                    break  
            else:  
            #print(sayı)
                asal.append(sayı)
    print(asal)
print("prime first:")
prime_first()
print("prime second:")
prime_second()
